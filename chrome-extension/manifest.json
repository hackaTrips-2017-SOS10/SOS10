{
    "manifest_version": 2,
    "name": "SOS10",
    "description": "Displays TST info.",
    "version": "0.2",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*",
        "geolocation"
    ]
}
