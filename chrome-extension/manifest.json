{
    "manifest_version": 2,
    "name": "SOS10",
    "description": "Displays TST info.",
    "version": "0.2",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "http://*/*", 
        "https://*/*",
        "geolocation"
    ]
}
